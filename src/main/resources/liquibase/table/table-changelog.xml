<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">

    <preConditions>
        <dbms type="postgresql"/>
    </preConditions>

    <include file="queue_tasks.sql" relativeToChangelogFile="true"/>

    <include file="users.sql" relativeToChangelogFile="true"/>
    <include file="sessions.sql" relativeToChangelogFile="true"/>

    <include file="chats.sql" relativeToChangelogFile="true"/>
    <include file="messages.sql" relativeToChangelogFile="true"/>
    <include file="updates.sql" relativeToChangelogFile="true"/>
    <include file="notifications.sql" relativeToChangelogFile="true"/>

</databaseChangeLog>